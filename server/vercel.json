{
  "version": 2,
  "framework": null,
  "functions": {
    "server/index.js": {
      "maxDuration": 10
    }
  },
  "scripts": {
    "start": "node --env-file=.env index.js"
  },
  "routes": [
    {
      "src": "/auth/google",
      "dest": "index.js"
    },
    {
      "src": "/auth/refresh-token",
      "dest": "index.js"
    },
    {
      "src": "/(.*)",
      "dest": "index.js"
    }
  ]
}
